 [];\n    productList.map((id, i) => {\n        result = result.concat(info.filter((x) => x.id == productList[i]));\n    });\n    let qtyUpdate, cartList;\n\n    useEffect(() => {\n        dispatch(actionCreator.cart(result, qt));\n        dispatch({\n            type: actionType.update,\n            list: productList,\n        });\n\n        // dispatch(actionCreator.qty(qt));\n    }, [qtyUpdate]);\n    // let total = ;\n    cartList = useSelector((state) => state.data.cart);\n    qtyUpdate = useSelector((state) => state.data.qty);\n    // console.log(qtyUpdate.reduce((x, y) => x + y, 0));\n\n    return (\n        <div>\n            <h3> Product List </h3>\n\n            <Table striped bordered hover size=\"sm\">\n                <thead>\n                    <tr>\n                        <th>#ID</th>\n                        <th>Title</th>\n                        <th>Price</th>\n                        <th>Quantity</th>\n                        <th>Remove</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {cartList.map((x, i) => (\n                        <tr key={x.id}>\n                            <td>{x.id}</td>\n\n                            <td>\n                                <img src={x.image} width=\"50\" height=\"40\" />{\" \"}\n                                {x.title}\n                            </td>\n                            <td>{x.price * 81} BDT</td>\n                            <td>{qtyUpdate[i]}</td>\n                            <Button\n                                variant=\"primary\"\n                                onClick={() => {\n                                    dispatch(\n                                        actionCreator.qtyUp(i, [...qtyUpdate])\n                                    );\n                                    dispatch({\n                                        type: actionType.removeUpdate,\n                                        value: 1,\n                                    });\n                                }}\n                                className=\"far fa-minus-square btn-sm\"\n                            ></Button>\n                            <Button\n                                variant=\"primary\"\n                                onClick={() => {\n                                    dispatch(\n                                        actionCreator.cartUp(\n                                            cartList,\n                                            x.id,\n                                            qtyUpdate,\n                                            i\n                                        )\n                                    );\n                                    dispatch({\n                                        type: actionType.remove,\n                                        value: qtyUpdate[i],\n                                        id: x.id,\n                                    });\n                                }}\n                                className=\"fas fa-trash-alt ml-3 btn-sm\"\n                            ></Button>\n                        </tr>\n                    ))}\n                </tbody>\n            </Table>\n            <Container>\n                <Row>\n                    <Col>\n                        <h4> Total Price</h4>\n                    </Col>\n                    <Col xs={{ offset: 6 }}>\n                        <h4>= {PriceCount()}</h4>\n                    </Col>\n                </Row>\n                <Row>\n                    <Col>\n                        <Link\n                            xs={{ span: 6, offset: 3 }}\n                            to=\"/\"\n                            className=\"btn btn-primary btn-lg\"\n                        >\n                            Continue Shopping\n                        </Link>\n                    </Col>\n                </Row>\n            </Container>\n        </div>\n    );\n}\n\nexport default Cart;\n","/mnt/home2/react_projects/react-ecommerce/src/components/Nav.js",[],"/mnt/home2/react_projects/react-ecommerce/src/store/reducers/cartReducer.js",["80"],"import * as actionType from \"../actions/actions\";\nconst initialState = {\n    value: 0,\n    productId: [],\n};\n\nfunction cartReducer(state = initialState, action) {\n    switch (action.type) {\n        case actionType.add:\n            return {\n                ...state,\n                value: state.value + 1,\n                productId: state.productId.concat(action.id),\n            };\n        case actionType.update:\n            return {\n                ...state,\n                productId: action.list,\n            };\n        case actionType.remove:\n            return {\n                ...state,\n                value: state.value - action.value,\n                productId: state.productId.filter((x) => x != action.id),\n            };\n        case actionType.removeUpdate:\n            return {\n                ...state,\n                value: state.value - action.value,\n            };\n        default:\n            return state;\n    }\n}\nexport default cartReducer;\n","/mnt/home2/react_projects/react-ecommerce/src/store/reducers/index.js",[],"/mnt/home2/react_projects/react-ecommerce/src/customHandler/findQuantity.js",[],"/mnt/home2/react_projects/react-ecommerce/src/store/actions/actions.js",[],"/mnt/home2/react_projects/react-ecommerce/src/store/actions/actionCreators.js",["81","82"],"import * as actionType from \"./actions\";\nimport axios from \"axios\";\n\nexport const cart = (data, qty) => {\n    return {\n        type: actionType.cart,\n        cart: data,\n        qty: qty,\n    };\n};\n\nexport const cartUp = (data, id, qty, i) => {\n    let dataUp = data.filter((y) => y.id != id);\n    //   let index = data.list.indexOf(id);\n    let qtyUp = qty.filter((m, n) => n != i);\n\n    return {\n        type: actionType.cartUp,\n        cart: dataUp,\n        qty: qtyUp,\n  